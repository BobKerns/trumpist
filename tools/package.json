    }
  ],
  "name": "trumpist",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bobkens/trumpist.git"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "node": true,
      "es6": true
    },
    "parserOptions": {
      "ecmaVersion": 2018
    }
  },
  "watch": {
    "build": {
      "patterns": [
        "dbs/*/*.tsx?",
        "dbs/*/test/*.tsx?",
        "*/*.tsx?"
      ],
      "extensions": [
        ".js",
        ".ts",
        ".js"
      ]
    }
  },
  "jest": {
    "transform": {
      "^(?!.+\\.d\\.test).+\\.tsx?$": "ts-jest",
      "^.+\\.d\\.test\\.ts$": "dts-jest/transform"
    },
    "testRegex": "\\.test\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "reporters": [
      "default",
      "dts-jest/reporter"
    ]
  },
  "types": [
    "@types/"
  ],
  "scripts": {
    "test": "bin/jest",
    "path": "echo PATH=$PATH",
    "doc": "bin/build-doc",
    "preview": "npm run doc",
    "view": "open ../docs/api/index.html",
    "build": "tsc --project tsconfig.json",
    "api": "../bin/publish-api"
  },
  "version": "1.0.0"
}
